<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.palladium.healthcenter">

    <joined-subclass name="Patient" table="patient" extends="Person">
        <key column="patient_id" not-null="true" on-delete="cascade" />
        <property name="patientId" type="int" not-null="true" column="patient_id" update="false" insert="false" />
        <many-to-one name="creator" class="User" column="creator"/>
        <property name="voided" type="java.lang.Boolean" column="voided"/>
    </joined-subclass>

</hibernate-mapping>
